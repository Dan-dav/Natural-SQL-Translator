name: Natural-SQL-Translator
version: 0.1
category: Natural Language Processing
build-type: Simple
cabal-version: >=1.10


executable Main
  hs-source-dirs:
    .
  main-is: Main.hs
  other-modules:
    DBCountries
    -- Generated by `gf -make -f haskell DBCountriesEng.gf DBCountriesSQL.gf`
  build-depends:
    base,
    gf
  default-language: Haskell2010
